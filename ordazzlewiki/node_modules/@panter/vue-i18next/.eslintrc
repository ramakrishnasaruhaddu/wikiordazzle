{
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module",
    "ecmaFeatures": {
      "experimentalObjectRestSpread": true,
      "globalReturn": true
    }
  },
  "globals": {
    "document": true,
    "window": true,
    "__VERSION__": true
  },
  "plugins": ["prettier"],
  "extends": ["airbnb", "prettier"],
  "rules": {
    "prettier/prettier": ["error", { "singleQuote": true }],
    "arrow-body-style": [1, "as-needed"],
    "no-underscore-dangle": 0,
    "react/prop-types": 0,
    "consistent-return": 0,
    "import/no-unresolved": 0,
    "import/extensions": 0,
    "import/no-extraneous-dependencies": 0
  }
}
